generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum WorkType {
  it_engineer
  idol
}

model WorkHistory {
  id        Int      @id @default(autoincrement())
  start_on  DateTime
  end_on    DateTime?
  content   String
  work_type WorkType
  point     Boolean
}

model AccessCounter {
  id        Int @id @default(autoincrement())
  count     Int
}
